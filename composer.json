{
    "name": "Momento blog",
    "description": "Nice looking, powerful blog written on php 7.0",
    "version": "0.0.1",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "blog",
        "php",
        "bootstrap"
    ],
    "homepage": "http://breezegames.cloudapp.net",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-form": "2.10.2",
        "zendframework/zend-eventmanager": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Momento\\": "module/Momento/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MomentoTest\\": "module/Momento/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}
